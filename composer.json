{
    "name": "titi60/notify-when-available",
    "type": "sylius-plugin",
    "description": "A Sylius plugin that enables the option of setting a future date when the product will be available. Users can add an email to notify then when the product can be purchased",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "sylius/sylius": "v1.9.4 || ~1.10.0 || dev-master"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^1.0",
        "friends-of-behat/cross-container-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.0",
        "friends-of-behat/variadic-extension": "^1.0",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^3.2",
        "phpstan/phpstan-shim": "^0.9.2",
        "phpunit/phpunit": "^5.6",
        "se/selenium-server-standalone": "^2.52",
        "sylius-labs/coding-standard": "^1.0",
        "symplify/easy-coding-standard": "^2.4",
        "sensio/generator-bundle": "^3.1"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Titi60\\SyliusNotifyWhenAvailablePlugin\\": "src/",
            "Tests\\Acme\\SyliusExamplePlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/app/AppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
